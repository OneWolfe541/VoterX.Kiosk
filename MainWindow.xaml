<Window x:Class="VoterX.Kiosk.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VoterX.Kiosk"
        mc:Ignorable="d"
        Title="VoterX"
        Height="740" Width="1366"
        MinHeight="740" MinWidth="1366"
        WindowStyle="None" ResizeMode="CanResizeWithGrip" AllowsTransparency="True"
        MouseDown="Window_MouseDown"
        KeyDown="Window_KeyDown"
        MouseMove="Window_MouseMove"
        Icon="C:\Program Files\VoterX\Images\AES.ico">

    <Grid Name="MainWindowGrid" MouseDown="MaximizeMyWindow">
        <Frame Name="MainWindowFrame" NavigationUIVisibility="Hidden" Navigated="MainFrame_Navigated" />
    </Grid>
</Window>
